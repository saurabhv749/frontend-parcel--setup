{
  "public": "public",
  "cleanUrls": true,
  "headers": [
    {
      "source": "**/*.@(jpg|jpeg|gif|webp|png|html|css|js)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=6604800"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; child-src 'none';"
        }
      ]
    }
  ]
}
